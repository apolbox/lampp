lampp.service()
{
    local options=${1}
    local modules=${2}

    case ${options} in
        'start' )
            sudo /etc/init.d/${modules} ${options}
            ;;

        'stop' )
            sudo /etc/init.d/${modules} ${options}
            ;;
    esac
}

lampp.accessLog()
{
    print.printTable ' ' "$(cat /var/log/apache2/access.log)"
}
